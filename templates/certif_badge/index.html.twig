{% extends 'base.html.twig' %}

{% block title %}CertifBadge index{% endblock %}

{% block body %}
    <h1>CertifBadge index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for certif_badge in certif_badges %}
            <tr>
                <td>{{ certif_badge.id }}</td>
                <td>
                    <a href="{{ path('app_certif_badge_show', {'id': certif_badge.id}) }}">show</a>
                    <a href="{{ path('app_certif_badge_edit', {'id': certif_badge.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_certif_badge_new') }}">Create new</a>
{% endblock %}
