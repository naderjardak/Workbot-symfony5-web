{% extends 'utilisateur/Dashbord/base.html.twig' %}
{% block body %}
    <div class="row" style="display: flex;align-items: center; justify-content: end;margin-top: 120px;">
    <div class="col-lg-9">
        <div class="col-11">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Liste des offes : {{ nb }}</h5>

                    <!-- Default Table -->
                    <table class="table">
                        <thead>
                        <tr>
                            <th>Titre</th>
                            <th>Typeoffre</th>
                            <th>description</th>
                            <th>Mode de travail</th>
                            <th>Date d'ajout</th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for f in oo %}



                            <tr>
                                <td>{{f.titre}}</td>
                                <td>{{f.typeoffre}}</td>
                                <td>{{f.description}}</td>
                                <td>{{f.modetravail}}</td>
                                <td>{{f.dateajout}}</td>

                        {% endfor %}
                        </tbody>
                    </table>


                </div>
            </div>
        </div>
    </div>

{% endblock %}